    process(A, Sel)
    begin
    if (A = '1') then
        S0 &lt;= (NOT Sel(2)) AND (NOT Sel(1)) AND (NOT Sel(0));
        S1 &lt;= (NOT Sel(2)) AND (NOT Sel(1)) AND Sel(0);
        S2 &lt;= (NOT Sel(2)) AND Sel(1) AND (NOT Sel(0));
